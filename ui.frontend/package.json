    "aempack": "^1.1.0",
    "autoprefixer": "^9.2.1",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
