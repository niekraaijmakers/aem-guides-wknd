    "storybook": "start-storybook -p 6006",
    "start-aempack": "node scripts/aempack.js",
    "start-aempack-pub": "cross-env PROXYHOST=local.wknd.com AEMPORT=4503 cross-env NODE_ENV=development npm-run-all -p start-aempack"
